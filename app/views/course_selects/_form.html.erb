    <%= form.label :name %>
    <%= form.text_field :name, id: :course_select_name %>
  </div>

  <div class="field">
    <%= form.label :score %>
    <%= form.number_field :score, id: :course_select_score %>
  </div>

  <div class="field">
    <%= form.label :tutor %>
    <%= form.text_field :tutor, id: :course_select_tutor %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
