<%= form_with(model: course_select, local: true) do |form| %>
  <% if course_select.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(course_select.errors.count, "error") %> prohibited this course_select from being saved:</h2>

      <ul>
      <% course_select.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :num %>
    <%= form.text_field :num, id: :course_select_num %>
  </div>

